@model IEnumerable<testDMS.Models.DONATION>

@{
    ViewBag.Title = "Index";
    Layout = null;
}

<table class="table">
    <tr>
        <th>
            <select class="form-control" style="width:auto;" id="Type" name="Type">
                <option value="">Choose Type</option>
                <option value="Pie Chart">Pie Chart</option>
                <option value="Line Graph">Line Graph</option>
                <option value="Bar Graph">Bar Graph</option>
                <option value="Histogram">Histogram</option>
                <option value="Original">Original</option>
            </select>
        </th>
        <th>
            <select class="form-control" style="width:auto;" id="Type" name="Type">
                <option value="">Choose Type</option>
                <option value="Pie Chart">Pie Chart</option>
                <option value="Line Graph">Line Graph</option>
                <option value="Bar Graph">Bar Graph</option>
                <option value="Histogram">Histogram</option>
                <option value="Original">Original</option>
            </select>
        </th>
        <th>
            <select class="form-control" style="width:auto;" id="Type" name="Type">
                <option value="">Choose Type</option>
                <option value="Pie Chart">Pie Chart</option>
                <option value="Line Graph">Line Graph</option>
                <option value="Bar Graph">Bar Graph</option>
                <option value="Histogram">Histogram</option>
                <option value="Original">Original</option>
            </select>
        </th>
        <th>
            <select class="form-control" style="width:auto;" id="Type" name="Type">
                <option value="">Choose Type</option>
                <option value="Pie Chart">Pie Chart</option>
                <option value="Line Graph">Line Graph</option>
                <option value="Bar Graph">Bar Graph</option>
                <option value="Histogram">Histogram</option>
                <option value="Original">Original</option>
            </select>
        </th>
    </tr>
    <tr>
        <th>
            @Html.DisplayName("Donor")
        </th>
        <th>
            @Html.DisplayName("Amount")
        </th>
        <th>
            @Html.DisplayName("Date")
        </th>
        <th>
            @Html.DisplayName("Department")
        </th>
        <th>
            @Html.DisplayName("GL Code")
        </th>
    </tr>

@foreach (var item in Model) { 
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DONOR.FNAME)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Amount)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DateRecieved)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CODE.Department)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CODE.GL)
            </td>
         </tr>
}
</table>



